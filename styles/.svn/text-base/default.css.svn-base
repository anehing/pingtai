.d2 {
border:1px solid #ACB6DD;
color:#325C87;
margin:10px auto 0;
overflow:hidden;
width:98%;
}
.d2 ul {
margin:3px auto;
width:99%;
}
.d2 ul li {
display:inline;
float:left;
height:24px;
line-height:22px;
list-style-type:none;
margin:2px 20px;
overflow:visible;
white-space:nowrap;
}
.an4 {
background:transparent url(../images/an4a.gif) repeat scroll 0 0;
border:0 none;
color:#305EA9;
cursor:pointer;
height:20px;
width:46px;
}
.tb_ {
background:transparent url(../images/d2_0.png) repeat-x scroll center bottom;
height:26px;
margin:5px auto 0;
overflow:visible;
padding:0;
width:98%;
}

.tb_ ul {
float:left;
margin-left:10px;
overflow:hidden;
width:90%;

}

.tb_ li {
cursor:pointer;
display:inline;
float:left;
height:17px;
line-height:17px;
list-style-type:none;
margin:0 2px;
padding:4px 20px;
}
.hovertab {
background:transparent url(../images/d2_1.png) repeat scroll 0 0;
border-left:1px solid #7297B4;
border-right:1px solid #7297B4;
border-top:1px solid #7297B4;
}
.normaltab {
background:transparent url(../images/d2_2.png) repeat scroll 0 0;
color:#434E67;
}

